{
    "cleanUrls": true,
    "installCommand": "npm install",
    "trailingSlash": true,

    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}